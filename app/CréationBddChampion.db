CREATE TABLE champions (
    id SERIAL PRIMARY KEY,
    id_champ INT,
    key VARCHAR(255),
    name VARCHAR(255),
    title VARCHAR(255),
    resource VARCHAR(255),
    attack_type VARCHAR(255),
    health_flat FLOAT,
    health_perLevel FLOAT,
    healthRegen_flat FLOAT,
    healthRegen_perLevel FLOAT,
    mana_flat FLOAT,
    mana_perLevel FLOAT,
    manaRegen_flat FLOAT,
    manaRegen_perLevel FLOAT,
    armor_flat FLOAT,
    armor_perLevel FLOAT,
    magicResistance_flat FLOAT,
    magicResistance_perLevel FLOAT,
    attackDamage_flat FLOAT,
    attackDamage_perLevel FLOAT,
    movespeed_flat FLOAT,
    acquisitionRadius_flat FLOAT,
    selectionRadius_flat FLOAT,
    pathingRadius_flat FLOAT,
    gameplayRadius_flat FLOAT,
    criticalStrikeDamage_flat FLOAT,
    criticalStrikeDamageModifier_flat FLOAT,
    attackSpeed_flat FLOAT,
    attackSpeed_perLevel FLOAT,
    attackSpeedRatio_flat FLOAT,
    attackCastTime_flat FLOAT,
    attackTotalTime_flat FLOAT,
    attackDelayOffset_flat FLOAT,
    attackRange_flat FLOAT,
    roles VARCHAR(255)[],
    ability_p TEXT[],
    ability_q TEXT[],
    ability_w TEXT[],
    ability_e TEXT[],
    ability_r TEXT[],
    role VARCHAR(255)
);
